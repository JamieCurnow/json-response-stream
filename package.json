{
  "name": "json-response-stream",
  "version": "1.0.2",
  "description": "Utility function for streaming JSON from LLM libraries such as Genkit",
  "author": "Jamie Curnow <jamie@island-web.ca> (https://github.com/JamieCurnow)",
  "homepage": "https://github.com/JamieCurnow/json-response-stream",
  "repository": "github:JamieCurnow/json-response-stream",
  "license": "MIT",
  "keywords": [
    "genkit",
    "llm",
    "json",
    "response",
    "stream"
  ],
  "main": "dist/index.cjs.cjs",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "dev": "tsc && vite build --watch",
    "start": "vite --host --open",
    "build": "tsc && vite build",
    "build:types": "dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "lint:scripts": "eslint ./src --ext .ts",
    "format:scripts": "prettier ./src --write"
  },
  "devDependencies": {
    "@types/node": "^22.8.1",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^3.0.9"
  }
}
